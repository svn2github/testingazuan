	<SERVICE name="TestPage2" type="PAGE">
		<VALIDATION blocking="false" validators="">
			<CONDITIONS>
				<PARAMETER name="MESSAGE" scope="SERVICE_REQUEST" value="DATA_DI_NASCITA"/>
			</CONDITIONS>
			<FIELDS>
				<FIELD name="field">
					<VALIDATOR validatorName="MANDATORY"/>
					<VALIDATOR validatorName="DATE" arg0="dd/MM/yyyy"/>
				</FIELD>	
			</FIELDS>
		</VALIDATION>
		<VALIDATION blocking="false" validators="">
			<CONDITIONS>
				<PARAMETER name="MESSAGE" scope="SERVICE_REQUEST" value="COMUNE_DI_NASCITA"/>
			</CONDITIONS>
			<FIELDS>
				<FIELD name="field">
					<VALIDATOR validatorName="MANDATORY"/>
					<VALIDATOR validatorName="ALFANUMERIC"/>
					<VALIDATOR validatorName="MINLENGTH" arg0="3"/>
					<VALIDATOR validatorName="MAXLENGTH" arg0="20"/>
				</FIELD>	
			</FIELDS>
		</VALIDATION>
		<VALIDATION blocking="false" validators="">
			<CONDITIONS>
				<PARAMETER name="MESSAGE" scope="SERVICE_REQUEST" value="IMPORTO_IN_EURO_SENZA_SIMBOLO"/>
			</CONDITIONS>
			<FIELDS>
				<FIELD name="field">
					<VALIDATOR validatorName="MANDATORY"/>
					<VALIDATOR validatorName="NUMERIC"/>
					<VALIDATOR validatorName="DECIMALS" arg0="2" arg1=","/>
					<VALIDATOR validatorName="NUMERICRANGE" arg0="0" arg1="1000000"/>
				</FIELD>	
			</FIELDS>
		</VALIDATION>
		<VALIDATION blocking="false" validators="">
			<CONDITIONS>
				<PARAMETER name="MESSAGE" scope="SERVICE_REQUEST" value="IMPORTO_IN_EURO_CON_SIMBOLO"/>
			</CONDITIONS>
			<FIELDS>
				<FIELD name="field">
					<VALIDATOR validatorName="MANDATORY"/>
					<VALIDATOR validatorName="REGEXP" arg0="^\s*(EURO|Euro)\s*(0|([1-9]([0-9])*))(,\d\d)?$"/> <!-- Maybe it's not correct, it's only for test! -->
				</FIELD>	
			</FIELDS>
		</VALIDATION>
				<VALIDATION blocking="false" validators="">
			<CONDITIONS>
				<PARAMETER name="MESSAGE" scope="SERVICE_REQUEST" value="PAROLA_CHIAVE"/>
			</CONDITIONS>
			<FIELDS>
