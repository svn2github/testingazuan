					<VALIDATOR validatorName="MANDATORY"/>
					<VALIDATOR validatorName="LETTERSTRING"/>
					<VALIDATOR validatorName="STRINGRANGE" arg0="abcd" arg1="abcf"/>
				</FIELD>	
			</FIELDS>
		</VALIDATION>
	</SERVICE>
			
