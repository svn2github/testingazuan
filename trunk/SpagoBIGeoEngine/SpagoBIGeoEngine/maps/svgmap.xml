<?xml version="1.0" encoding="UTF-8"?>
<MAPS>
    <!-- type:svg|jpeg -->
	<MAP name="abitanti_provincia_venezia" enabled="TRUE" type="svg">
		<MAP_PROVIDER class_name="it.eng.geo.map.DefaultMapProvider"
		              uri="http://localhost:8080/SpagoBIGeoEngine/img/maps/ve_comuni.svg"
					  xml_parser_impl="it.eng.geo.document.XercesXMLDocument"/>
		<DATAMART_PROVIDER class_name="it.eng.geo.datamart.DefaultDatamartProvider" 
		                   registered_pool_name="spagobigeo" 
		                   query="select path_id ,num_abitanti from path_id_abitanti pa,abitanti a where a.cod_istat = pa.cod_istat"
		                   element_id="path_id"
		                   element_name="num_abitanti"/>
		<CONFIGURATION>
			<!-- x,y: position of the first element of the legend  -->
			<LEGEND x="1700000" y="-5080000" width="10000" height="5000" style="stroke:black;stroke-width:100">
				<TITLE description="Abitanti provincia di Venezia" style="font-family:verdana;font-size:150em;font-weight:bold;fill:black"/>
			<!-- ordered by threshold -->
				<LEVELS>
					<LEVEL threshold="0" style="fill:blue;fill-opacity:0.1">
						<TEXT description="Meno di 1000" style="font-family:verdana;font-size:90em;fill:blue;fill-opacity:0.1" />
					</LEVEL>
					<LEVEL threshold="1000" style="fill:blue;fill-opacity:0.2">
						<TEXT description="Da 1000 a 2000" style="font-family:verdana;font-size:90em;fill:blue;fill-opacity:0.2" />
					</LEVEL>
					<LEVEL threshold="2000" style="fill:blue;fill-opacity:0.3">
						<TEXT description="Da 2000 a 3000" style="font-family:verdana;font-size:90em;fill:blue;fill-opacity:0.3" />
					</LEVEL>
					<LEVEL threshold="3000" style="fill:blue;fill-opacity:0.4">
						<TEXT description="Da 3000 a 4000" style="font-family:verdana;font-size:90em;fill:blue;fill-opacity:0.4" />
					</LEVEL>
					<LEVEL threshold="4000" style="fill:blue;fill-opacity:0.5">
						<TEXT description="Da 4000 a 5000" style="font-family:verdana;font-size:90em;fill:blue;fill-opacity:0.5" />
					</LEVEL>
					<LEVEL threshold="5000" style="fill:blue;fill-opacity:0.6">
						<TEXT description="Da 5000 a 10000" style="font-family:verdana;font-size:90em;fill:blue;fill-opacity:0.6" />
					</LEVEL>
					<LEVEL threshold="10000" style="fill:blue;fill-opacity:0.7">
						<TEXT description="Da 10000 a 20000" style="font-family:verdana;font-size:90em;fill:blue;fill-opacity:0.7" />
					</LEVEL>
					<LEVEL threshold="20000" style="fill:blue;fill-opacity:0.8">
						<TEXT description="Da 20000 a 50000" style="font-family:verdana;font-size:90em;fill:blue;fill-opacity:0.8" />
					</LEVEL>
					<LEVEL threshold="50000" style="fill:blue;fill-opacity:0.9">
						<TEXT description="Da 50000 a 100000" style="font-family:verdana;font-size:90em;fill:blue;fill-opacity:0.9" />
					</LEVEL>
					<LEVEL threshold="100000" style="fill:blue;fill-opacity:1">
						<TEXT description="Piu' di 100000" style="font-family:verdana;font-size:90em;fill:blue;fill-opacity:1" />
					</LEVEL>
				</LEVELS>
			</LEGEND>
		</CONFIGURATION>
	</MAP>
</MAPS>